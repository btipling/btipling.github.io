{
    "compilerOptions": {
        "target": "es5",
        "module": "esNext",
        "moduleResolution": "node",
        "sourceMap": true,
        "alwaysStrict": true,
        "strictFunctionTypes": false,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "lib": [
            "dom",
            "dom.iterable",
            "es5",
            "es2015",
            "es2016",
            "es2017"
        ]
    }
}
