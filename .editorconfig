# EditorConfig is awesome: http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.js]
charset = utf-8
indent_style = space
indent_size = 4

[*.ts]
charset = utf-8
indent_style = space
indent_size = 4


[*.json]
charset = utf-8
indent_style = space
indent_size = 4
